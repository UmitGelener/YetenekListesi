@using Skill_CodeFirstEntityProject.Models.Siniflar
@model List<YETENEKLER>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutNavbar.cshtml";
}
<!-----------------Admin Kısmı---------------------------->
<div class="container">
    <table class="table table-bordered">
        <tr>
            <th>ID</th>
            <th>YETENEK</th>
            <th>DEĞER</th>
            <th>SİL</th>
            <th>GÜNCELLE</th>
        </tr>
        @foreach (var x in Model)
        {
            <tr>
                <td>@x.ID</td>
                <td>@x.YetenekAciklama</td>
                <td>@x.YetenekYüzdeDeger</td>
                <td>@Html.ActionLink("Sil","YetenekSil", "Admin",new { id = x.ID},new { @class="btn btn-danger"})</td>                
                <td>@Html.ActionLink("Güncelle", "YetenekGuncelle", "Admin", new { id = x.ID }, new { @class="btn btn-success"})</td>                
            </tr>
        }
    </table>
</div>
